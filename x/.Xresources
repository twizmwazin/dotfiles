rofi.color-enabled:	true

! State:            'bg',         'fg',         'bgalt',      'hlfbg',        'hlfg'
rofi.color-normal:	t_background, t_foreground, t_background, t_selection, t_foreground
rofi.color-urgent:	t_background, t_foreground, t_background, t_selection, t_foreground
rofi.color-active:	t_background, t_foreground, t_background, t_selection, t_foreground

!                   'background', 'border'
rofi.color-window:	t_background, t_foreground

rofi.font:           SourceCodePro-Regular 13
rofi.fuzzy:	         true
rofi.hide-scrollbar: true
rofi.lines:	         15
rofi.opacity:        75
rofi.bw:             0
