!TomorrowNight
#define t_background       #282828
#define t_current_line     #ebdbb2
#define t_selection        #a89984
#define t_foreground       #ebdbb2
#define t_comment          #969896
#define t_red              #cc241d
#define t_orange           #d79921
#define t_yellow           #fabd2f
#define t_green            #98971a
#define t_aqua             #689d6a
#define t_blue             #458558
#define t_purple           #b16286


*.foreground:   t_foreground
*.background:   t_background
*.cursorColor:  #aeafad

! Black / Grey
*.color0:       #282828
*.color8:       #928374

! Red / Bright Red
*.color1:       t_red
*.color9:       #fb4934

! Green + Bright Green
*.color2:       t_green
*.color10:      #b8bb26

! Yellow (Orange) + Bright Yellow (Yellow)
*.color3:       t_orange
*.color11:      t_yellow

! Blue + Bright Blue
*.color4:       t_blue
*.color12:      #83a598

! Magenta (Purple) + Bright Magenta
*.color5:       t_purple
*.color13:      #d3869b

! Cyan (Aqua) + Bright Cyan
*.color6:       t_aqua
*.color14:      #8ec07c

! Light Grey (Selection) + White (Current Line)
*.color7:       t_selection
*.color15:      t_current_line

